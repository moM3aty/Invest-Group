                    <div class="property-card-new h-100">
                        <div class="property-card-new__image-container">
                            <img src="@property.MainImageUrl" class="property-card-new__image" alt="@property.TitleEn">
                        </div>
                        <div class="property-card-new__content">
                            <h3 class="property-card-new__title" data-en="@property.TitleEn" data-ar="@property.TitleAr">@property.TitleEn</h3>
                            <p class="property-card-new__location" data-en="@property.LocationEn" data-ar="@property.LocationAr">
                                <i class="fas fa-map-marker-alt me-2"></i>@property.LocationEn
                            </p>
                            <p class="property-card-new__description"
                               data-en="@(property.OverviewEn != null && property.OverviewEn.Length > 150 ? property.OverviewEn.Substring(0, 150) + "..." : property.OverviewEn)"
                               data-ar="@(property.OverviewAr != null && property.OverviewAr.Length > 150 ? property.OverviewAr.Substring(0, 150) + "..." : property.OverviewAr)">
                                @(property.OverviewEn != null && property.OverviewEn.Length > 150 ? property.OverviewEn.Substring(0, 150) + "..." : property.OverviewEn)
                            </p>

                            @if (!string.IsNullOrEmpty(property.PaymentPlanEn))
                            {
                                <div class="property-card-new__features">
                                    @foreach (var item in property.PaymentPlanEn.Split('|', System.StringSplitOptions.RemoveEmptyEntries).Take(3))
                                    {
                                        <span><i class="fas fa-hand-holding-usd"></i>@item</span>
                                    }
                                </div>
                            }

                            <div class="m-auto">
                                <a asp-action="Details" asp-route-id="@property.Id" class="thm-btn">
                                    <em data-ar="اكتشف المزيد" data-en="Discover More">Discover More</em>
                                    <span><i class="fas fa-arrow-right"></i></span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>

